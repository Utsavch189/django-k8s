{
    "builds": [{
            "src": "app/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "265mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "static"
            }
        }
    ],
    "routes": [{
        "src": "/(.*)",
        "dest": "app/wsgi.py"
    }]
}